##Создание таблицы Students
CREATE TABLE Students (
Student_Id INTEGER,
Student_Name TEXT,
School_Id INTEGER PRIMARY KEY
);

##Наполнение таблицы данными
INSERT INTO Students (Student_Id, Student_Name, School_Id)
VALUES (201, 'Иван', 1),
(202, 'Петр', 2),
(203, 'Анастасия', 3),
(204, 'Игорь', 4);

##Запрос информации по ID студента
SELECT s.Student_Id AS 'ID Студента',
s.Student_Name AS 'Имя студента',
s.School_Id AS 'ID школы',
t.School_Name AS 'Название школы'
FROM Students AS s
JOIN Teacher AS t ON s.School_Id = t.School_Id
WHERE s.Student_Id = 202; -- указываем ID студента, по которому хотим получить информацию
